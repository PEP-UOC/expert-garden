{
	"cli": {
		"version": ">= 0.52.0",
		"requireCommit": true
	},
	"build": {
		"androidAPK": {
			"extends": "base",
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_VARIANT": "androidAPK"
			}
		},
		"iosSimulator": {
			"extends": "base",
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			},
			"env": {
				"APP_VARIANT": "iosSimulator"
			}
		},
		"development": {
			"extends": "base",
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"extends": "base",
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "preview"
			}
		},
		"production": {
			"extends": "base",
			"env": {
				"APP_VARIANT": "expertgarden"
			}
		},
		"base": {
			"node": "16.13.2",
			"npm": "8.1.2",
			"yarn": "1.22.17"
		}
	},
	"submit": {
		"production": {}
	}
}
